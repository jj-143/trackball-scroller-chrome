{
  "name": "trackball-scroller",
  "version": "2.0.0",
  "description": "Trackball Scroller for Chrome",
  "scripts": {
    "dev": "ts-node parcel/build.mts dev",
    "dev:all": "ts-node parcel/build.mts all",
    "build": "ts-node ./parcel/build.mts production",
    "build:prod": "npm run build",
    "postbuild:prod": "npm run zip",
    "zip": "cd ./build && zip -r ../Trackball-Scroller.zip ./ && cd ..",
    "e2e": "cypress run --headed",
    "test": "jest --config=./jest.config.js",
    "test:watch": "jest --watch --config=./jest.config.js",
    "test:e2e": "concurrently -k npm:dev:all npm:e2e",
    "cypress": "cypress open",
    "prettier": "prettier --write -c ./src/**/*.ts"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@parcel/config-default": "^2.9.1",
    "@parcel/config-webextension": "^2.9.1",
    "@parcel/core": "^2.9.1",
    "@types/chrome": "^0.0.236",
    "@types/jest": "^26.0.0",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "babel-jest": "^29.5.0",
    "concurrently": "^8.0.1",
    "cypress": "^12.8.1",
    "cypress-real-events": "^1.7.6",
    "eslint": "^8.36.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.0.5",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "wait-for-expect": "^3.0.2"
  },
  "repository": "https://github.com/jj-143/trackball-scroller-chrome",
  "author": "<jj.dev.001@gmail.com>",
  "license": "MIT"
}
