{
  "name": "trackball-scroller",
  "version": "0.0.3",
  "description": "",
  "dependencies": {},
  "devDependencies": {
    "parcel-bundler": "^1.12.4"
  },
  "scripts": {
    "build": "parcel build *.js *.html *.css --no-source-maps ",
    "prebuild": "[[ -d dist ]] && rm -rf dist || true",
    "postbuild": "npm run copy-static",
    "build:prod": "npm run build",
    "postbuild:prod": "npm run zip",
    "copy-static": "cp -r ./static ./manifest.json icon.png ./dist",
    "zip": "cd ./dist && zip -r ../Trackball-Scroller.zip ./ && cd .."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/jj-143/trackball-scroller.git"
  },
